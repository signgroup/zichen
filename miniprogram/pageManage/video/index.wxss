.secname{ font-size: 17px; line-height: 56rpx}
.seccode{ font-size: 13px; color: rgba(89,89,89,.6); line-height: 1.2;}


.stay_bottom{ height: 50px; }
.bottom_row{ width: 100%; overflow: hidden; background-color: #fff; line-height: 49px; height: 49px; border-top: 1px #a5a5a5 solid; position: fixed; z-index: 99; left: 0; bottom: 0; font-size: 17px; color: #929292;}

.stay_bottom .all_select  .ico_checker{background-position: 0 -17px;}
.hidden,.none{display: none!important;}
.video-box{
     position: relative; z-index: 199;
}
.icon-cover{
     width:120rpx;
     height: 80rpx;
     border-radius: 6rpx;
}

.fixed-button {
     position: fixed;
     bottom: 0;
     left: 0;
     width: 100%;
     z-index: 999;
     background-color: #fff;
     padding: 20rpx 30rpx;
     display: flex;
     box-sizing: border-box;
}

.fixed-button button {
     flex: 1;
}

.primary {
     position: absolute;
     left: 5rpx;
     top: 5rpx;
     z-index: 100;
     -webkit-clip-path: polygon(50% 0%, 100% 38%, 82% 100%, 18% 100%, 0% 38%);
     clip-path: polygon(50% 0%, 100% 38%, 82% 100%, 18% 100%, 0% 38%);
     width: 60rpx;
     height: 60rpx;
     background-color: rgba(0, 0, 0, .3);
     text-align: center;
     line-height: 60rpx;
}
